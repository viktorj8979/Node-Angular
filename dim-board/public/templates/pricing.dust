        <script>
            $(function() {
                $( "#slider" ).slider({
                  range: "min",
                  value: 1,
                  min: 1,
                  max: 1000,
                  slide: function( event, ui ) {
                    var numUsers = ui.value;
                    var price = 20;
                    if(numUsers > 999) {
                        price = 5;
                    } else if(numUsers > 499) {
                        price = 7;
                    } else if(numUsers > 99) {
                        price = 9;
                    } else if(numUsers > 49) {
                        price = 12;
                    } else if(numUsers > 9) {
                        price = 15;
                    }
                    $( "#users" ).text(numUsers);
                    $( "#amount" ).text( "$" + (numUsers * price));
                  }
                });
                $( "#users" ).text(1);
                $( "#amount" ).text( "$" + $( "#slider" ).slider( "value" ) * 20 );
              });
        </script>
